#include <iostream>
#include <string>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Ru");
	int k = 0;
	bool l = 0;
	bool o = 0;
	string edin[] = { " one ", " two ", " three ", " four ", " five ", " six ", " seven ", " eight ", " nine " };
	string teen[] = { " ten ", " eleven "," twelve ", " thirteen ", " fourteen ", " fifteen ", " sixteen ", " seventeen ", " eighteen ", " nineteen " };
	string des[] = { " ", " twenty ", " thirty ", " forty ", " fifty ", " sixty ", " seventy ", " eightty ", " ninety " };
	string a1, a2, a3, a4, a5, a6, a7, a8, a9, a10, a11, a12 = "";
	string str;
	cout << "Введите число: " << endl;
	cin >> str;
	unsigned long int x = stoi(str);
	int a = x;
	while (a != 0)
	{
		a = (a / 10);
		k++;
	}

	int g = 0;
	if ((x % 100 <= 19) && (x % 100 >= 10))
		g = 1;

	if ((k >= 1) and (g == 0))
	{
		int t;
		t = x % 10;
		if (t != 0)
			a1 = a1 + edin[t - 1];
	}
	if (k >= 2)
	{
		int t;
		t = (x / 10) % 10;
		if ((g == 0) and (t != 0))
		{
			a2 = a2 + des[t - 1];
		}
		else if (t != 0)
		{
			t = x % 10;
			a2 = a2 + teen[t];
		}
	}
	if (k >= 3)
	{
		int t;
		t = (x / 100) % 10;
		if (t != 0)
			a3 = a3 + edin[t - 1] + "hundred";
	}

	int d = 0;
	if ((x / 1000 <= 19) && (x / 1000 >= 10))
		d = 1;

	if ((k >= 4) and (d == 0))
	{

		int t;
		t = (x / 1000) % 10;
		if (t != 0) {
			a4 = a4 + edin[t - 1];
			o = 1;
		}

	}
	if (k >= 5)
	{

		int t;
		t = (x / 10000) % 10;
		if ((d == 0) and (t != 0))
		{
			o = 1;
			a5 = a5 + des[t - 1];
		}
		else if (t != 0)
		{
			t = (x / 1000) % 10;
			a5 = a5 + teen[t];
			o = 1;
		}
	}
	if (k >= 6)
	{

		int t;
		t = (x / 100000) % 10;
		if (t != 0) {
			o = 1;
			a6 = a6 + edin[t - 1] + "hundred";
		}
	}
	int p = 0;
	if ((x / 1000000 <= 19) && (x / 1000000 >= 10))
		p = 1;
	if ((k >= 7) and (p == 0))
	{
		l = 1;
		int t;
		t = (x / 1000000) % 10;
		if (t != 0)
			a7 = a7 + edin[t - 1];
	}

	if (k >= 8)
	{
		l = 1;
		int t;
		t = (x / 10000000) % 10;
		if ((p == 0) and (t != 0))

		{
			a8 = a8 + des[t - 1];
		}
		else if (t != 0)
		{
			t = (x / 1000000) % 10;
			a8 = a8 + teen[t];
		}
	}
	if ((k >= 9) and (p == 0))
	{
		l = 1;
		int t;
		t = (x / 100000000) % 10;
		if (t != 0)
			a9 = a9 + edin[t - 1] + "hundred";
	}


	if (k == 10)
	{
		int t;
		t = (x / 1000000000) % 10;
		a10 = a10 + edin[t - 1] + "billion";
	}

	if (l == 1)
		a11 = a9 + " " + a8 + " " + a7 + "million";
	if (o == 1)
		a12 = a6 + " " + a5 + " " + a4 + "thousand";
	cout << a10 << " " << a11 << " " << a12 << " " << a3 << a2 << a1 << endl;

	system("pause");
	return 0;
}
